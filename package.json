{
  "name": "poshlina-dev",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "homepage": "https://casebookkiller.github.io/poshlina-dev",
  "scripts": {
    "deploy": "gh-pages -d dist",
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "build:watch": "tsc --noEmit --watch && vite build",
    "update:qrscan": "rsync -avr --progress ../react-qr-scanner/dist/. ./node_modules/@yudiel/react-qr-scanner/dist --exclude .git --exclude node_modules",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "preview": "vite preview",
    "predeploy": "npm run build"
  },
  "dependencies": {
    "@liquid-js/qr-code-styling": "file:../qr-code-styling",
    "@liquid-js/qrcode-generator": "^1.0.1",
    "@telegram-apps/sdk": "^2.4.0",
    "@telegram-apps/sdk-react": "^2.0.9",
    "@telegram-apps/telegram-ui": "^2.1.8",
    "@tonconnect/ui-react": "^2.0.9",
    "@xmldom/xmldom": "^0.9.5",
    "antd": "^5.22.2",
    "antd-mobile": "^5.38.1",
    "browser-image-tools": "file: ./node_modules/@liquid-js/qr-code-styling/tools",
    "dotenv": "^16.4.5",
    "eruda": "^3.4.0",
    "file-type": "^19.6.0",
    "primereact": "^10.8.4",
    "react": "^18.3.1",
    "react-bootstrap-icons": "^1.11.4",
    "react-camera-pro": "^1.4.0",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@telegram-apps/mate": "^0.0.24",
    "@types/dom-webcodecs": "^0.1.13",
    "@types/node": "^22.7.4",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@types/react-loadable": "^5.5.11",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "@vitejs/plugin-basic-ssl": "^1.1.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@yudiel/react-qr-scanner": "^2.0.8",
    "eslint": "^9.11.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "gh-pages": "^6.1.1",
    "globals": "^15.10.0",
    "qrcode": "^1.5.4",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.7.0",
    "vite": "^5.4.8",
    "vite-tsconfig-paths": "^5.0.1"
  }
}
